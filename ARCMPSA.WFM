                                             && leave shell(.F.)
        shell(.f.)
   *  endif
   close procedure SampProc.prg, Buttons.cc
    if .not. empty(ARCMPS->order_no)
   select ARCMPDUP
   seek ARCMPS->order_no+dtos(ARCMPS->order_date)
   if found() .and. .not. empty(post_date)
   select ARCMPS
   replace post_date with date()
   endif
   endif

   close databases
