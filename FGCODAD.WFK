      if l4 = "00" .and. lerr 
             ? "Error here 10"
             endif
          
 if left(l7,1) = "P"  .and. .not. (l4  = "00" .and. l5 = "00" ;
    .and. l6 = "00" .and. l2 = "1") .and. (l3 = "1" .or. l3 = "3") && Island 1 and 3 only
