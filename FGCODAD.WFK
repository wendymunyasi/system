   endif
      if form.updated 
      select fgcoy
      FORM.MOUSEPOINTER = 11
      go top
      if .not. eof()
    do FGCODs_rtn WITH FORM.BLUSER,FORM.BLEVEL
    CLOSE TABLES
   * DO NGLPARM.PRG
     endif
     endif
     close procedure SAMPProc.prg, ;
     buttons.cc
   close databases
   
